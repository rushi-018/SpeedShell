{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "cleanUrls": true,
    "framework": null,
    "builds": [
      { "src": "index.html", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  